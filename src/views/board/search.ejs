<%- <%- include("bm_input") %>

<!-- 검색 결과를 출력하는 부분 추가 -->
<div class="search-results">
    <h2>검색 결과</h2>
    <% if (results.length === 0) { %>
        <p>검색 결과가 없습니다.</p>
    <% } else { %>
        <ul>
            <% results.forEach(result => { %>
                <li><a href="/board/data/<%= result.BOARD_ID %>"><%= result.BOARD_TITLE %></a></li>
            <% }); %>
        </ul>
    <% } %>
</div>