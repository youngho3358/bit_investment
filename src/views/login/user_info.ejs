                        <a href="/register" class="top_register_button">회원가입</a>
                    <%}%>
                </div>
            </div>
        </div>
        <div class="container">
            <div class="info_box">
                <form action="" method="post">
                    <div class="info_text"><b>내&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;정&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;보</b></div>
                    <div class="info_input_box">
                        
                    </div>
                </form>
            </div>
            <div><%=member.member_id%></div>
            <div><%=member.email%></div>
            <div><%=member.name%></div>
            <div><%=member.age%></div>
            <div><%=member.addr%></div>
            <div><%=member.phone%></div>
            <div><%=member.nickname%></div>
            <div><%=member.grade%></div>
            <div><%=member.login_type%></div>
            <div><%=member.id%></div>
        </div>
    </div>
</body>
</html>